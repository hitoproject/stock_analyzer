<div class="row justify-content-around mt-3">
    <div class="list-group col-6 col-xl-4 p-3">
        <h3 class="list-group-item list-group-item-secondary">Recent Search</h3>
        <% for (var i = 0 ; i < 5 ; i++){ %>
            <a href='/<%= code_recent[i]%>' class="list-group-item list-group-item-action"><%= code_recent[i]%></a>
        <% } %>
    </div>

    <div class="list-group col-6 col-xl-4 p-3">
        <h3 class="list-group-item list-group-item-secondary">Freqency Searched</h3>
        <% for (var i = 0 ; i < 5 ; i++){ %>
            <a href='/<%= code_freqency[i][0]%>' class="list-group-item list-group-item-action d-flex justify-content-between align-item-center">
                <%= code_freqency[i][0]%>
                <span class="badge badge-primary badge-pill"><%= code_freqency[i][1] %></span>
            </a>
        <% } %>
    </div>
</div>
